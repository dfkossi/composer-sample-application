{
  "name": "letter-of-credit",
  "version": "0.0.1",
  "private": false,
  "author": "peniel winchester",
  "description": "TPE lettre de credit pour le commerce",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run licchk",
    "eject": "react-scripts eject",
    "licchk": "license-check-and-add"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "letter-of-credit-network": "^0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-stepper-horizontal": "^1.0.10",
    "react-toggle": "^4.0.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-sass": "^3.2.1",
    "license-check-and-add": "~2.3.0"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules",
      "Dockerfile"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [],
    "insert_license": false,
    "license_formats": {
      "gitignore": {
        "append": "\n",
        "eachLine": {
          "prepend": "# "
        }
      },
      "js|ts|scss|css": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  }
}
